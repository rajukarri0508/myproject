<!-- =========================
     NAVIGATION LINKS     
============================== -->
<div class="navbar navbar-fixed-top custom-navbar" role="navigation">
    <div class="container">
  
      <!-- navbar header -->
      <div class="navbar-header">
        
        <a href="#" class="navbar-brand">The Food Freaks</a>
      
      </div>
      <div class="collapse navbar-collapse">
  
        
        <ul class="nav navbar-nav navbar-right">
          <li><a data-toggle="modal"  href="#myModal">Login/Registration</a></li>
          
           
        </ul>
  
      </div>
  
    </div>
  </div>
  
  
  
  <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel"aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div  >
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
          <a href="/"><img src="c.png" width="45px" height="45px" class="pull-right"></a>
          <center><h4 class="modal-title" id="myModalLabel">Login/Registration</h4></center>
        </div>
        <div class="modal-body">
          <div class="row">
            <div class="col-md-8" style="border-right: 1px dotted #C2C2C2;padding-right: 30px;">
                                          <!-- Nav tabs -->
              <ul class="nav nav-tabs">
              <li><a href="#Registration" data-toggle="tab">Registration</a></li>
              <li class="active"><a href="#Login" data-toggle="tab">Login</a></li>
              </ul>
                                          <!-- Tab panes -->
              <div class="tab-content">
                <div class="tab-pane active" id="Login">
                  <form class="form-horizontal" action="/authuser" method="POST" data-toggle="validator" role="form">
                    <div class="form-group has-feedback">
                      <label for="username" class="col-lg-2 control-label">Username</label>
                      <div class="col-lg-4">
                        <input type="text" class="form-control" name="username"   placeholder="Username"  required>
                        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                      <div class="help-block with-errors"></div>
                      </div>
                    </div><br/>
                    <div class="form-group has-feedback">
                      <label for="inputpassword" class="col-lg-2 control-label">Password</label>
                      <div class="col-lg-4">
                        <input type="password" name="password" data-minlength="8" class="form-control" id="inputpassword" placeholder="password" data-error="please enter perfect password" required>
                        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                        <div class="help-block with-errors"></div>
                        </div>
                    </div><br>
                    <div class="row">
                      <div class="col-sm-10">
                       <a href="/indexlogged"> <button type="submit" class="btn btn-primary btn-sm">Login</button></a>
                          <a href="/forgotPassword">Forgot  password?</a>
                      </div>
                    </div>
                  </form>
                </div>
                <div class="tab-pane" id="Registration">
                  <div class="container" id="signup">
                    <form class="form-horizontal" action="/addowner" method="POST" data-toggle="validator" role="form">
                      <fieldset>
                          <div class="form-group has-feedback">
                              <label for="inputusername" class="col-lg-2 control-label">Title</label>
                                <div class="col-lg-4">
                                  <input type="text" class="form-control" name="name" pattern="^[_A-z0-9\s]{1,}$" id="inputusername" placeholder="Title" data-error="Please enter name field." required>
                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                          <div class="help-block with-errors"></div>
                          
                        </div>
                        
                                        </div><br>

                        <div class="form-group has-feedback">
                          <label for="inputusername" class="col-lg-2 control-label">Username</label>
                            <div class="col-lg-4">
                              <input type="text" class="form-control" name="title" pattern="^[_A-z0-9\s]{1,}$" id="inputusername" placeholder="Username" data-error="Please enter name field." required>
                                        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                      <div class="help-block with-errors"></div>
                      
                    </div>
                    
                                    </div><br>
  
  
                                        <div class="form-group has-feedback">
                                              <label for="inputpassword" class="col-lg-2 control-label">Password</label>
                                              <div class="col-lg-4">
                                                <input type="password" name="password" data-minlength="8" class="form-control" id="inputpass" placeholder="password" data-error="please enter perfect password" required>
                                                 <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                           <div class="help-block with-errors"></div>
                        </div>
                                            </div><br>
                        <div class="form-group has-feedback">
                                              <label class="col-lg-2 control-label">ConfirmPassword</label>
                                              <div class="col-lg-4">
                                                <input type="password" class="form-control" id="inputpasswordconfirm" data-match="#inputpass" data-match-error="Whoops, these don't match" placeholder="ConfirmPassword" required>
                           <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                           <div class="help-block with-errors"></div>
                        </div>
                                            </div><br>
  
                                            <div class="form-group has-feedback">
                                              <label for="inputEmail"  class="col-lg-2 control-label">Email</label>
                                              <div class="col-lg-4">
                                                <input type="email" name="username" class="form-control" placeholder="Email"  required>
                      <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                      <div class="help-block with-errors"></div>
                                              </div>
                                            </div><br>
                            
                            <div class="form-group has-feedback">
                                                  <label for="inputEmail" class="col-lg-2 control-label">Mobile</label>
                                                  <div class="col-lg-4">
                                                    <input type="number"  name="number" data-minlength="10" pattern="^[0-9]\d{2,4}-\d{6,8}$"  class="form-control" id="inputmobile" placeholder="Number" data-error="please enter the correct mobile number"  required>
                                                    <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                            <div class="help-block with-errors"></div>
                          </div>
                                                </div><br>
  
                         <div class="form-group has-feedback">
                                                      <label for="inputEmail" class="col-lg-2 control-label">Location</label>
                                                      <div class="col-lg-4">
                                                        <input type="text" name="location" class="form-control" id="inputEmail" placeholder="Location" required>
                              <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                            </div>
                                                    </div><br>
                                   
                                    
                                   
                                    <div class="form-group">
                                      <div class="col-lg-10 col-lg-offset-2">
                                        <button type="reset" class="btn btn-default">Reset</button>
                                        <button type="submit" class="btn btn-primary" href="#">Submit</button>
                                      </div>
                                    </div>
                                  </fieldset>
                              </form>
                  </div>
                </div>
              </div>
                <div id="OR" class="hidden-xs">OR</div>
              </div>
              <div class="col-md-4">
                <div class="row text-center sign-with">
                  <div class="col-md-12">
                    <h3>Sign in with</h3>
                  </div>
                  <div class="col-md-12">
                    <div class="btn-group btn-group-justified">
                      <a href="#" class="btn btn-primary">Facebook</a>
                      <a href="#" class="btn btn-danger">Google</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>                
  </div>



<!-- =========================
    HOME SECTION   
============================== -->
<section id="home" class="parallax-section">
	<div class="container">
		<div class="row">

			<div class="col-md-12 col-sm-12">
				
      				<div class="container my-auto">
        				<div class="row">
          					<div class="col-lg-10 mx-auto">
							
            						<h1 class="masthead text-center text-white text-uppercase">
              							<strong>THE FOOD FREAKS</strong>
            						</h1>
							
            						
          					</div>
          					<div class="col-lg-8 mx-auto">
            						<center><p class="text-faded mb-5">&nbsp;Grab the location of the food and snack trucks at your finger tips</p></center>
                    </div>
                    <form action="/search" method="GET">
                      <div>
                              <input type="text" name="location_search" placeholder="Search by Location..." class="form-control">
                              <button type="submit" class="btn btn-primary"> Search..</button>
                      </div>
              </form>
      </div>
                    </center>        				</div>
      				</div>
    				</header>
			</div>


		</div>
	</div>
</section>





<section id="bbbb">
    <div class="row" >
        <div class="col-md-12 col-sm-12 wow bounceIn">
            <div class="section-title">
             <center> <h2>Near Food Trucks</h2></center>
            </div>
        </div>
        
    </div>
  <div class="row text-center"  id="halls" style="display:flex ; flex-wrap : wrap ;">
    <% if ( noMatch != "") { %>
            <a href="/search" > Go Back </a>
            <h1><%= noMatch %></h1>
            
    <% } %>
    <% owners.forEach(function(i){ %>
        <% var n = i.images.length %>
    <div class="col-md-3 col-sm-6">
        <div class="thumbnail">
                <img src="upload/<%= i.images[n-1].filename %>">
                
        </div>
        <p>
          <a href="/service/<%= i._id %>/view" class="btn btn-primary"><i class="fas fa-chevron-circle-down"></i> View More </a>
      </p> 
    </div>
    <% }); %>
</div>
</section>

<section>
    <footer class="container-fluid text-center footer">
    <p style="font-size:20px; color:green">View the map</p>
  <div style="width: 100%">
  <iframe width="100%" height="220px" src="https://maps.google.com/maps?width=100%&amp;height=600&amp;hl=en&amp;q=<%= search %>+(Map)&amp;ie=UTF8&amp;t=&amp;z=14&amp;iwloc=B&amp;output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0">
  <a href="https://www.maps.ie/create-google-map/">Google Maps iframe generator</a></iframe></div><br />  
  
</footer> 
  </section>



